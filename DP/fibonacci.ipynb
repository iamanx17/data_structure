    "def fibb(n):\n",
    "    if n==0 or n==1:\n",
    "        return n\n",
    "    ans1=fibb(n-1)\n",
    "    ans2=fibb(n-2)\n",
    "    \n",
    "    myans=ans1+ans2\n",
    "    return myans\n",
    "\n",
    "n=int(input())\n",
    "ans=fibb(n)\n",
    "print(ans)"
   ]
